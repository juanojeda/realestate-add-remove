{
  "name": "boilerplate-min",
  "version": "1.1.0",
  "description": "A minimal react boilerplate for small projects",
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "start": "gulp serve",
    "start:prod": "gulp serve:prod",
    "build": "gulp && npm run"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.5.2",
    "browser-sync": "^2.2.1",
    "del": "^2.2.0",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^7.1.0",
    "expose-loader": "^0.7.3",
    "globals-loader": "0.0.3",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.1",
    "gulp-cache": "^0.4.2",
    "gulp-cssnano": "^2.0.0",
    "gulp-eslint": "^3.0.0",
    "gulp-iconfont": "^8.0.1",
    "gulp-iconfont-css": "^2.1.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^3.1.0",
    "gulp-sass-lint": "^1.3.2",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-uglify": "^2.1.0",
    "gulp-useref": "^3.0.0",
    "html-loader": "^0.4.4",
    "ngrok": "^2.2.6",
    "path": "^0.12.7",
    "run-sequence": "^1.2.2",
    "script-loader": "^0.7.0",
    "webpack": "^2.2.1",
    "webpack-stream": "^3.2.0",
    "wiredep": "^4.0.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
