    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "foundation-sites": "^6.3.0-rc1",
    "hammerjs": "^2.0.8",
    "imagemin-mozjpeg": "^6.0.0",
    "normalize-scss": "^6.0.0",
    "imagemin-pngquant": "^5.0.1",
    "lodash": "^4.16.4",
    "react": "^15.6.1",
    "react-dom": "^15.5.4",
    "sass-lint": "^1.12.1"
  }
}
